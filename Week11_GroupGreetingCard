<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
<script type="text/javascript">

	$("#scene1").hide();
	$("#scene2").hide();
	i = 1;
	function loop() {
		switch(i) {
			case 1:
				$("#scene2").hide();
				$("#scene1").show();
				i = 2;
				break;
			case 2:
				$("#scene1").hide();
				$("#scene2").show();
				i = 1;
				break;
		}
	}
	loop();
	setInterval(loop, 1000);


$(document).ready(function() {
	$("pre").fadeOut(0).fadeIn(3000)
})


</script>


